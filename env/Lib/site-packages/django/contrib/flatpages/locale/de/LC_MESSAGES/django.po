#: .\env\Lib\site-packages\django\contrib\flatpages\admin.py:13
msgid "Advanced options"
msgstr "Erweiterte Optionen"

#: .\env\Lib\site-packages\django\contrib\flatpages\apps.py:8
msgid "Flat Pages"
msgstr "Flat Pages"

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:11
#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:9
msgid "URL"
msgstr "Adresse (URL)"

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Beispiel: „/about/contact/“. Wichtig: Am Anfang und Ende muss ein "
"Schrägstrich („/“) stehen."

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Dieser Wert darf nur Buchstaben, Ziffern, Punkte, Unterstriche, "
"Bindestriche, Schrägstriche und Tilden enthalten."

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Beispiel: „/about/contact“. Wichtig: Am Anfang muss ein Schrägstrich („/“) "
"stehen."

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:47
msgid "URL is missing a leading slash."
msgstr "Der URL fehlt ein vorangestellter Schrägstrich."

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:52
msgid "URL is missing a trailing slash."
msgstr "Der URL fehlt ein abschließender Schrägstrich."

#: .\env\Lib\site-packages\django\contrib\flatpages\forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr ""
"Flatpage mit der URL %(url)s ist für die Website %(site)s bereits vorhanden"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:10
msgid "title"
msgstr "Titel"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:11
msgid "content"
msgstr "Inhalt"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:12
msgid "enable comments"
msgstr "Kommentare aktivieren"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:14
msgid "template name"
msgstr "Name des Templates"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Beispiel: „flatpages/contact_page.html“. Wenn dieses Feld nicht gesetzt ist, "
"wird standardmäßig „flatpages/default.html“ benutzt."

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:23
msgid "registration required"
msgstr "Registrierung erforderlich"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr ""
"Wenn hier ein Haken gesetzt ist, können nur angemeldete Benutzer die Seite "
"sehen."

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:29
msgid "sites"
msgstr "Websites"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:33
msgid "flat page"
msgstr "Flat Page"

#: .\env\Lib\site-packages\django\contrib\flatpages\models.py:34
msgid "flat pages"
msgstr "Flat Pages"
